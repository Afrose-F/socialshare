function share(e){console.log("Image: "+e.image);var t=require("dk.napp.social");if(console.log("Facebook available: "+t.isFacebookSupported()),console.log("Twitter available: "+t.isTwitterSupported()),t.isRequestTwitterSupported()){var n=[];t.addEventListener("accountList",function(e){console.log("Accounts:"),n=e.accounts,console.log(n)}),t.twitterAccountList()}var r;t.addEventListener("facebookAccount",function(e){console.log("facebookAccount: "+e.success),r=e.account,console.log(e)}),t.addEventListener("complete",function(e){if(console.log("complete: "+e.success),console.log(e),"activityView"==e.platform||"activityPopover"==e.platform)switch(e.activity){case t.ACTIVITY_TWITTER:console.log("User is shared on Twitter");break;case t.ACTIVITY_CUSTOM:console.log("This is a customActivity: "+e.activityName)}}),t.addEventListener("error",function(e){console.log("error:"),console.log(e)}),t.addEventListener("cancelled",function(e){console.log("cancelled:"),console.log(e)});var i={};i.text=e.status,e.image&&(i.image=e.image),t.isActivityViewSupported()?t.activityView(i):alert("Sorry...your phone appears to be old-ish...sorry")}exports.share=share;